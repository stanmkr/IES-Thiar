<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="codigo">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:pattern value="[A-Z]\d{3}[A-Z]{2}"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
</xs:schema>
